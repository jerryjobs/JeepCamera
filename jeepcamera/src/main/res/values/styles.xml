<resources>

  <!-- Base application theme. -->
  <style name="jeep_AppTheme"
      parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/jeep_colorPrimary</item>
    <item name="colorPrimaryDark">@color/jeep_colorPrimaryDark</item>
    <item name="colorAccent">@color/jeep_colorAccent</item>
  </style>

  <style name="jeep_AppTheme.NoActionBar">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
  </style>

  <style name="jeep_AppTheme.AppBarOverlay"
      parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

  <style name="jeep_AppTheme.PopupOverlay"
      parent="ThemeOverlay.AppCompat.Light"/>

</resources>
